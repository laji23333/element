<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.element.Dao.BusinessMapper">
    <insert id="saveOrder">
        insert into order (orderId,userId,businessId,orderDate,orderTotal,daId,orderState)
		 values (#{orderId},#{userId},#{businessId},#{orderDate},#{orderTotal},#{daId},#{orderState})
    </insert>
</mapper>